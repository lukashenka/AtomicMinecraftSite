<div class="divided">    
{% trans_default_domain 'FOSUserBundle' %}
        <h3>Сменить email</h3>
        <form action="{{ path('fos_user_profile_edit') }}" {{ form_enctype(edit_profile_form) }} method="POST" class="fos_user_profile_edit form-horizontal"  role="form">
    {{ form_start(edit_profile_form) }}
    {{ form_errors(edit_profile_form) }}


    {{ form_row(edit_profile_form.email, { 'attr': {'class': 'form-control'} }) }}

         {{ form_row(edit_profile_form.current_password, { 'attr': {'class': 'form-control'} }) }}
             {{ form_row(edit_profile_form.username) }}
 {{ form_row(edit_profile_form._token) }}

                <div>
                    <input type="submit" class="btn btn-default" value="{{ 'profile.edit.submit'|trans }}" />
                </div>




            </form>
        </div>