

<html>
    <head>

        <title>Atomic.im -> {% block title %} Minecraft сервер {% endblock title %}  </title>



        <meta name="viewport" content="width=device-width, initial-scale=1.0">

        {% block stylesheets %}

        <link rel="stylesheet" href="/lib/bootstrap/css/bootstrap.min.css">
        <link rel="stylesheet" href="/css/layout.css">
        <link rel="stylesheet" href="/css/style.css">
         {% endblock stylesheets %} 

         {% block stylesheets_additional %}
         {% endblock stylesheets_additional %} 

        {% javascripts  %}
        <script type="text/javascript" src="/js/jquery.min.js"></script>
        <script type="text/javascript" src="/lib/bootstrap/js/bootstrap.min.js"></script>
        {% endjavascripts %}

        {% block js_additional  %}
        {% endblock js_additional %}




    </head>
    <body>

        <div class="main-container container">
            <header>

                <div id="header">

                    <div class="row">
                        <div class="col-lg-12">

                            <div id="header-left" >
                               {% render controller('AtomicUserBundle:Security:renderLogin') %}
                                </div>


                                <div id="logo">
                                    <img src="/img/atomic-logo.png" alt="Atomic minecraft server"/>
                                </div>

                            </div>

                        </div>

                    </div>

                </header>

                <div id="atomic-menu">


               {% block navigation %}
                    <div class="navbar">

                        <div class="navbar-collapse collapse">

                            <ul class="nav navbar-nav">
                                 {% render controller('AtomicMainMenuBundle:Menu:show',{'currentRoute':app.request.attributes.get('_route')}) %}



                                </ul>

                            </div><!--/.navbar-collapse -->
                {% endblock %}
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-lg-12">

                            <div class="col-lg-8">
                                <div id="content">
              {% block content %} content {% endblock content %}

                                    </div>

                                </div>
                                <div class="col-lg-4">
                                    <div id="right-column">


                                        <div id="monitor" class="blog-item">
                                            <h4>Мониторинг</h4>

                                    {% render controller('AtomicServerBundle:Check:check') %} 
                                            <div id="vote" style="font-size: 12px;text-align: center;">Голосуй - не голосуй, все равно получишь ШиШ* 

                                                <a href="http://mc-top.org/project/214/view"><img src="http://mc-top.org/banner/214.png?type=2"></a>
                                                <a href="http://topcraft.ru/servers/1566"><img src="http://topcraft.ru/ban_img/1566"></a>
                                                <!--      <a href="http://www.leagueofgames.org/projects/139"><img src="http://www.leagueofgames.org/img/log_banner.png" width="88" height="31" alt="LoG" /></a> -->

                                            </div>
                                            <p  style="font-size: 10px;text-align: center;">  *ШиШ - игровая валюта </p>
                                        </div>

                                        <div id="last-users" class="blog-item">
                                            <h4>Новое пополнение</h4>
                                            <!--     <p>Адрес сервера - atomic.im</p>
                                               <p><a href="http://www.teamspeak.com/?page=downloads" target="_blank">Скачать teamspeak</a></p>
                                               <iframe width="280" height="200" src="//www.youtube.com/embed/QjJW3djOoEU" frameborder="0" allowfullscreen></iframe> -->
                                    {% render controller('AtomicUserBundle:Users:lastUser') %} 
                                        </div>

                                        <div id="vk" class="blog-item">
                                            <script type="text/javascript" src="//vk.com/js/api/openapi.js?104"></script>

                                            <!-- VK Widget -->
                                            <div id="vk_groups"></div>
                                            <script type="text/javascript">
                                                VK.Widgets.Group("vk_groups", {mode: 2, width: "277", height: "400"}, 60318843);
                                                </script>
                                            </div>
                                        </div>


                                    </div>
                                </div>

                            </div>







                            <footer>

                                <div id="footer">

                                    <div class="left-column">
                                        <div class="item">
                                            <!-- begin WebMoney Transfer : accept label -->
                                            <a href="http://www.megastock.ru/" target="_blank"><img src="/img/acc_white_on_transp_ru.png" alt="www.megastock.ru" border="0"></a>
                                        </div>

                                        <div class="item">
                                            <!-- begin WebMoney Transfer : attestation label -->
                                            <a href="https://passport.webmoney.ru/asp/certview.asp?wmid=224394073004" target="_blank"><img src="/img/v_white_on_transp_ru.png" alt="Здесь находится аттестат нашего WM идентификатора 224394073004" border="0" /></a>
                                            <!-- end WebMoney Transfer : attestation label -->

                                        </div>
                                        <div class="item"><a href="http://interkassa.com/" target="_blank">
                                                <img src="/img/ik_88x31_01.gif" title="Платежная система обслуживается компанией INTERKASSA" border="0">
                                            </a>
                                        </div>

                                        <div class="item">
                                            <!-- Yandex.Metrika counter -->
                                            <script type="text/javascript">
                                                (function(d, w, c) {
                                                    (w[c] = w[c] || []).push(function() {
                                                        try {
                                                            w.yaCounter22651156 = new Ya.Metrika({id: 22651156,
                                                                webvisor: true,
                                                                clickmap: true,
                                                                trackLinks: true,
                                                                accurateTrackBounce: true});
                                                        } catch (e) {
                                                        }
                                                    });

                                                    var n = d.getElementsByTagName("script")[0],
                                                            s = d.createElement("script"),
                                                            f = function() {
                                                        n.parentNode.insertBefore(s, n);
                                                    };
                                                    s.type = "text/javascript";
                                                    s.async = true;
                                                    s.src = (d.location.protocol == "https:" ? "https:" : "http:") + "//mc.yandex.ru/metrika/watch.js";

